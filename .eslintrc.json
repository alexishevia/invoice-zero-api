{
  "env": {
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "no-console": [0],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-restricted-syntax": [0],
    "no-param-reassign": [
      "error",
      {
        "ignorePropertyModificationsFor": ["memo"]
      }
    ],
    "no-continue": [0]
  }
}
